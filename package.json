{
  "name": "hn_sentiment",
  "version": "1.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "build:jsx": "jsx app/src/ app/build/ -x jsx",
    "build:bundle": "browserify app/build/app.js -o public/javascripts/bundle.js -v",
    "build": "npm run build:jsx && npm run build:bundle",
    "watch:jsx": "jsx -w app/src/ app/build/ -x jsx",
    "watch:bundle": "watchify app/build/app.js -o public/javascripts/bundle.js -v"
  },
  "dependencies": {
    "bluebird": "^3.2.1",
    "chroma-js": "^1.1.1",
    "csv-parser": "^1.9.2",
    "csv-stringify": "^1.0.1",
    "d3": "^3.5.0",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "firebase": "^2.3.2",
    "history": "^1.17.0",
    "jade": "~1.6.0",
    "morgan": "~1.3.0",
    "natural": "^0.2.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.3",
    "reactfire": "^0.5.1",
    "request": "^2.69.0",
    "sentiment": "^1.0.4"
  },
  "devDependencies": {
    "watchify": "latest",
    "browserify": "latest",
    "nodemon": "latest"
  },
  "author": "Chris Stadler"
}
